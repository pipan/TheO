            <string>///construct
event_inherited();
global.energyOscilatorPosibility = 0.1;
global.energySpawnTime = 10000;
global.energySpawnNumber = 2;
global.players = ds_list_create();
//arenaFull = instance_create(global.centerX, global.centerY, objectGappyCircle);
global.arena = instance_create(global.centerX, global.centerY, objectArena);
arenaCreate(global.arena, false);
ball = instance_create(global.centerX, global.centerY, objectBall);
vectorConstruct(ball.velocity, 5, 1, 0, 5, 10, 2);
//player 1
player = instance_create(global.centerX + global.innerR, global.centerY, objectPlayer);
playerCreate(player, global.arena.parts[| 0], spritePadOrange, getOrange(), 0);
setControll(player.anchor, vk_up, vk_down, vk_left, vk_right);
setScoreBoardXY(player.scoreBoard, global.centerX + global.outerR, global.centerY - global.outerR);
//player 1 abilities
playerAddAbility(player, objectAbilityBlink, ord("P"));
ds_list_add(global.players, player);
//player 2
player = instance_create(global.centerX - global.innerR, global.centerY, objectPlayer);
playerCreate(player, global.arena.parts[| 2], spritePadBlue, getBlue(), 180);
setControll(player.anchor, ord("W"), ord("S"), ord("A"), ord("D"));
setScoreBoardXY(player.scoreBoard, global.centerX - global.outerR, global.centerY - global.outerR);
ds_list_add(global.players, player);
//pridanie branky pre playera
global.arena.parts[| 0].belongs = global.players[| 0];
global.arena.parts[| 2].belongs = global.players[| 1];
time = current_time;
spawnEnergyTimeDiff = 0;
//pridanie menu pre pauzi
circleBorder = instance_create(room_width / 2, room_height/2, objectMMBorder);
circleBorder.render = false;
setCircleCenter(circleBorder, global.arena.centerPoint, global.outerR / 2);
    //menu items
    //index menu items group
    var group = instance_create(room_width / 2, room_height - global.outerR / 2, objectMMItemGroup);
    MMItemGroupConstruct(group, 1, 150, 15, 90, false, 0);
    setCircleCenter(group, global.arena.centerPoint, global.outerR + global.menuItemOffset);
    ds_list_add(global.menuGroups, group);
    //item 0 - resume
    item = instance_create(room_width / 2, room_height - global.outerR / 2 - global.menuItemOffset, objectGMResume);
    setCircleCenter(item, global.arena.centerPoint, global.outerR + global.menuItemOffset);
    setMMItemAngleByLength(item, group.itemWidth);
    item.group = group;
    ds_list_add(group.items, item);
    //item 1 - options
    var item = instance_create(room_width / 2, room_height - global.outerR / 2 - global.menuItemOffset, objectGMItemOptions);
    setCircleCenter(item, global.arena.centerPoint, global.outerR + global.menuItemOffset);
    setMMItemAngleByLength(item, 0);
    item.group = group;
    ds_list_add(group.items, item);
    //item 2 - main menu
    item = instance_create(room_width / 2, room_height - global.outerR / 2 - global.menuItemOffset, objectGMItemMainMenu);
    setCircleCenter(item, global.arena.centerPoint, global.outerR + global.menuItemOffset);
    setMMItemAngleByLength(item, -group.itemWidth);
    item.group = group;
    ds_list_add(group.items, item);
</string>
          </argument>
        </arguments>
      </action>
