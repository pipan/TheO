          <argument>
            <kind>1</kind>
            <string>///Input
var up = 0;
var goal_list = ds_list_create();
var goal = -1;
//set goal
if (isSetInput(self)){
   if (keyboard_check(controllUp)){
      ds_list_add(goal_list, 90);
   }
   if (keyboard_check(controllDown)){
      ds_list_add(goal_list, 270);
   }
   if (keyboard_check(controllLeft)){
      ds_list_add(goal_list, 180);
   }
   if (keyboard_check(controllRight)){
      if (keyboard_check(controllDown)){
           ds_list_add(goal_list, 360);
      }
      else{
           ds_list_add(goal_list, 0);
      }
   }
   if (ds_list_size(goal_list) &gt; 0){
      goal = 0;
      for (var i = 0; i &lt; ds_list_size(goal_list); i++){
          goal += goal_list[| i];
      }
      goal = goal / ds_list_size(goal_list);
   }
}
ds_list_destroy(goal_list);
//find path
if (goal &gt;= 0){
   var differance = image_angle - goal;
   if (differance &gt; 0){
      if (differance &lt;= 180){
         up = -1;
      }
      else if (differance &gt; 180){
          up = 1
      }
   }
   else if (differance &lt; 0){
        if (differance &gt;= -180){
           up = 1;
        }
        else if (differance &lt; -180){
             up = -1
        }
   }
}
if (up != 0){
   //klavesa hore
    if (up &gt; 0){
       setVectorNormByVector(acceleration, rotation, 1);
    }
    //klavesa dole
    else{
       setVectorNormByVector(acceleration, rotation, -1);
    }
   setVectorValue(acceleration, acceleration.defaultValue);
}
else{
   setVectorValue(acceleration, 0);
   if (velocity.value &gt; 0){
      if (velocity.value &lt; frictionMove){
         setVectorValue(velocity, 0);
      }
      else{
           setVectorValue(velocity, velocity.value - frictionMove);
      }
   }
}
</string>
